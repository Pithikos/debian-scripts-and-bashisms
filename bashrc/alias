# Changing directory
alias ~~='cd ~/workspace'
alias lastfile="ls -ldt ~/workspace/* ~/workspace/*/*  ~/workspace/*/*/* | head -n 1 | awk '{print \$8}'"
alias lastproj="lastfile | cut -d '/' -f -5"
alias work='~~'
alias wa="work && atom ."

# Listing files
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
